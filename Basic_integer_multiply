// This file create an integer variables, multiply then print the result.

#include <stdio.h>

int term; // term used in two expressions.

int main()
{
    term = 3 * 5;
    printf("Twice %d is %d.\n", term, term * 2);
    printf("Three times %d is %d.\n", term, term * 3);
    getchar();
    return(0);
}
